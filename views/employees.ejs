<br>
<h2>Employees</h2>

<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Name</th>
      </tr>
    </thead>
    
    <tbody>
        <% emp.forEach(function(e){%>
            <tr>
                <td><%= e.id %></td>
                <td><%= e.name %></td>
            </tr>
        <%})%>    

    </tbody>
  </table>